{
  "name": "redux-photo-gallery",
  "homepage": "https://torabiho.github.io/redux-photo-gallery/",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "gh-pages": "^3.1.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-photo-gallery": "^8.0.0",
    "react-redux": "^7.2.2",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2"
